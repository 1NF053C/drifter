generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./database/drifter.db"
}

model MapboxPublicConfig {
  id  Int @id @default(autoincrement())
  publicKey String
  startLng Float
  startLat Float
  zoomLevel Int
}

model ShoeStore {
  id    Int @id @default(autoincrement())
  name  String
  streetAddress String
  lng   Float
  lat   Float
}
